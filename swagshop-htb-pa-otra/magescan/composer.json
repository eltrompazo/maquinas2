{
    "name": "steverobbins/magescan",
    "description": "Scan a Magento application for information",
    "license": "CC-BY-4.0",
    "authors": [
        {
            "name": "Steve Robbins",
            "email": "steve@steverobbins.com"
        }
    ],
    "require": {
        "symfony/console": "~2.6",
        "steverobbins/magento-version-identification": "1.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "1.1.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "bin": ["bin/magescan"],
    "replace": {
        "steverobbins/magento-guest-audit": "*"
    }
}
